services:
  aspintroduction:
    build:
      context: /Users/radoslavbogdanov/Downloads/DEVELOPMENT/C# WEB/ASP.NET Introduction/AspIntroduction
      dockerfile: AspIntroduction/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: AspIntroduction
      target: base
    container_name: AspIntroduction
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: aspintroduction:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/AspIntroduction.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - /Users/radoslavbogdanov/Downloads/DEVELOPMENT/C# WEB/ASP.NET Introduction/AspIntroduction/AspIntroduction:/app:rw
    - /Users/radoslavbogdanov/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/radoslavbogdanov/.aspnet/https:/root/.aspnet/https:ro
    - /Users/radoslavbogdanov/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /Users/radoslavbogdanov/.nuget/packages:/root/.nuget/packages:ro
    - /Users/radoslavbogdanov/Downloads/DEVELOPMENT/C# WEB/ASP.NET Introduction/AspIntroduction:/src:rw
version: '3.4'